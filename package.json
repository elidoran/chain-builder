{
  "name": "chain-builder",
  "version": "0.7.1",
  "description": "Manage an array of functions and execute them in a series with a variety of flows.",
  "main": "index.js",
  "author": "Eli Doran <eli@elidoran.com>",
  "license": "MIT",
  "scripts": {
    "compile": "coffee -c lib",
    "clean": "rm -rf ./lib/*.js",
    "prepublish": "coffee --bare --compile lib",
    "postpublish": "rm -rf ./lib/*.js",
    "testrun": "mocha --compilers coffee:coffee-script/register --reporter spec --bail --check-leaks test",
    "test010": "nave use 0.10 npm run testrun",
    "test012": "nave use 0.12 npm run testrun",
    "test4": "nave use 4.2 npm run testrun",
    "test-all": "npm run test010 && npm run test012 && npm run test4",
    "test": "if [ -e $TRAVIS ]; then npm run test-all; else npm run testrun; fi"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/elidoran/chain-builder.git"
  },
  "bugs": "http://github.com/elidoran/chain-builder/issues",
  "devDependencies": {
    "coffee-script": "^1.10.0",
    "mocha": "^2.3.3",
    "nave": "^0.5.3"
  },
  "dependencies": {}
}
