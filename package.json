{
  "name": "chain-builder",
  "version": "0.12.0",
  "description": "Manage an array of functions and execute them in a series with a variety of flows.",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "keywords": [
    "command",
    "chain",
    "pipeline",
    "filter",
    "execution",
    "execute",
    "run",
    "series",
    "sequence",
    "control",
    "flow",
    "synchronous",
    "asynchronous",
    "pause",
    "resume"
  ],
  "author": "Eli Doran <eli+npm@elidoran.com>",
  "license": "MIT",
  "scripts": {
    "lint": "coffeelint -f build/coffeelint.json lib",
    "build": "coffee --bare --compile lib",
    "clean": "rm -rf ./lib/*.js",
    "prepublish": "npm run build",
    "postpublish": "npm run clean",
    "test": "mocha --compilers coffee:coffee-script/register --reporter spec --bail --check-leaks test",
    "test012": "nave use 0.12 npm test",
    "test4": "nave use 4.8 npm test",
    "test6": "nave use 6.10 npm test",
    "test7": "nave use 7.9 npm test",
    "test-all": "npm run test012 && npm run test4 && npm run test6 && npm run test7",
    "precoverage": "npm run clean",
    "coverage": "istanbul cover -x 'build/**' _mocha -- --compilers coffee:coffee-script/register --require ./build/coverage.js -R spec test",
    "publish-coverage": "npm run coverage && cat coverage/lcov.info | coveralls",
    "precoverage-lcov": "npm run clean",
    "coverage-lcov": "istanbul cover -x 'build/**' _mocha --report lcovonly -- --compilers coffee:coffee-script/register --require ./build/coverage.js -R spec test/",
    "coverage-clean": "rm -rf ./coverage"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/elidoran/chain-builder.git"
  },
  "bugs": "http://github.com/elidoran/chain-builder/issues",
  "devDependencies": {
    "coffee-coverage": "^2.0.1",
    "coffee-script": "^1.12.5",
    "coffeelint": "^1.16.0",
    "coveralls": "^2.13.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nave": "^2.2.3"
  },
  "dependencies": {
    "array-flatten": "^2.1.1"
  }
}
