{
  "name": "chain-builder",
  "version": "0.11.2",
  "description": "Manage an array of functions and execute them in a series with a variety of flows.",
  "main": "lib",
  "keywords": [
    "command",
    "chain",
    "pipeline",
    "filter",
    "execution",
    "execute",
    "run",
    "series",
    "sequence",
    "control",
    "flow",
    "synchronous",
    "asynchronous",
    "pause",
    "resume"
  ],
  "author": "Eli Doran <eli+npm@elidoran.com>",
  "license": "MIT",
  "scripts": {
    "build": "coffee --bare --compile lib",
    "clean": "rm -rf ./lib/*.js",
    "prepublish": "npm run build",
    "postpublish": "npm run clean",
    "test": "mocha --compilers coffee:coffee-script/register --reporter spec --bail --check-leaks test",
    "test012": "nave use 0.12 npm test",
    "test4": "nave use 4.8 npm test",
    "test6": "nave use 6.10 npm test",
    "test7": "nave use 7.9 npm test",
    "test-all": "npm run test012 && npm run test4 && npm run test6 && npm run test7"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/elidoran/chain-builder.git"
  },
  "bugs": "http://github.com/elidoran/chain-builder/issues",
  "devDependencies": {
    "coffee-script": "^1.12.5",
    "mocha": "^3.2.0",
    "nave": "^2.2.3"
  },
  "dependencies": {
    "array-flatten": "^2.1.1"
  }
}
