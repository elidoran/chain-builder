{
  "name": "chain-builder",
  "version": "0.13.2",
  "description": "Manage an array of functions and execute them in a series with a variety of flows.",
  "main": "lib/index.js",
  "files": [
    "lib/index.js",
    "lib/chain.js",
    "lib/control.js",
    "lib/get-options.js"
  ],
  "keywords": [
    "command",
    "chain",
    "pipeline",
    "filter",
    "execution",
    "execute",
    "run",
    "series",
    "sequence",
    "control",
    "flow",
    "synchronous",
    "asynchronous",
    "pause",
    "resume"
  ],
  "author": "Eli Doran <eli+npm@elidoran.com>",
  "license": "MIT",
  "scripts": {
    "lint": "coffeelint -f build/coffeelint.json lib",
    "build": "coffee --bare --compile lib",
    "clean": "rm -rf ./lib/*.js",
    "prepublish": "npm run build",
    "postpublish": "npm run clean",
    "test": "mocha --compilers coffee:coffee-script/register --reporter spec --bail --check-leaks test",
    "test4": "nave use 4.8 npm test",
    "test6": "nave use 6.10 npm test",
    "test7": "nave use 7.10 npm test",
    "test-all": "npm run test4 && npm run test6 && npm run test7",
    "precoverage": "npm run clean",
    "coverage": "istanbul cover -x 'build/**' _mocha -- --compilers coffee:coffee-script/register --require ./build/coverage.js -R spec test",
    "precoverage-lcov": "npm run clean",
    "coverage-lcov": "istanbul cover -x 'build/**' _mocha --report lcovonly -- --compilers coffee:coffee-script/register --require ./build/coverage.js -R spec test/",
    "benchmark": "coffee benchmark/index.coffee --nodejs '--turbo' -- --report",
    "benchmark-save": "coffee benchmark/index.coffee --nodejs '--turbo' -- --store"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/elidoran/chain-builder.git"
  },
  "bugs": "http://github.com/elidoran/chain-builder/issues",
  "devDependencies": {
    "@optimal/fn": "^0.1.0",
    "benchmark": "^2.1.4",
    "chalk": "^1.1.3",
    "coffee-coverage": "^2.0.1",
    "coffee-script": "^1.12.6",
    "coffeelint": "^1.16.0",
    "comma-number": "^2.0.0",
    "console.table": "^0.8.0",
    "coveralls": "^2.13.1",
    "deep-eql": "^2.0.2",
    "istanbul": "^0.4.5",
    "mocha": "^3.4.1",
    "mocha-lcov-reporter": "^1.3.0",
    "nave": "^2.2.3",
    "pad": "^1.1.0"
  },
  "dependencies": {
    "@flatten/array": "^1.1.1"
  },
  "engines": {
    "node": ">=4"
  }
}
